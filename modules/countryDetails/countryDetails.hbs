<div class="modal-content -background-image -background-{{id}}-v" data-id="{{id}}">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h4 class="modal-title" data-i18n="values:countries.{{ISO}}">{{Country}}</h4>
    </div>
    <div class="modal-body">
        <div class="clearfix">
            <ul class="nav nav-pills col-md-2">
                <li class="active">
                    <a href="#country_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-flag"></span>
                    <span class="hidden-xs" data-i18n="sections:country.link"></span>
                    </a>
                </li>
                <!--
                    <li>
                      <a href="#climate_pane" data-toggle="tab">
                      <span class="glyphicon glyphicon-cloud"></span>
                      <span class="hidden-xs" data-i18n="sections:climate.link"></span>
                      </a>
                    </li>
                -->
                <li>
                    <a href="#population_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-user"></span>
                    <span class="hidden-xs" data-i18n="sections:population.link"></span>
                    </a>
                </li>
                <li>
                    <a href="#standards_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-random"></span>
                    <span class="hidden-xs" data-i18n="sections:standards.link"></span></a>
                </li>
                <!--
                    <li>
                      <a href="#spending_pane" data-toggle="tab">
                      <span class="glyphicon glyphicon-usd"></span>
                      <span class="hidden-xs" data-i18n="sections:spending.link"></span>
                      </a>
                    </li>
                    -->
                {{#bli}}
                <li>
                    <a href="#life_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-heart"></span>
                    <span class="hidden-xs" data-i18n="sections:life.link"></span>
                    </a>
                </li>
                {{/bli}}
                {{#citizenships}}
                <li>
                    <a href="#travel_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-plane"></span>
                    <span class="hidden-xs" data-i18n="sections:travel.link"></span>
                    </a>
                </li>
                {{/citizenships}}
                <li>
                    <a href="#score_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-ok"></span>
                    <span class="hidden-xs" data-i18n="sections:score.link"></span>
                    </a>
                </li>
                <li>
                    <a href="#links_pane" data-toggle="tab">
                    <span class="glyphicon glyphicon-link"></span>
                    <span class="hidden-xs" data-i18n="sections:links.link"></span>
                    </a>
                </li>
            </ul>
            <div class="tab-content col-md-10">
                <section id="score_pane" class="tab-pane fade">
                    <p><span data-i18n="countryDetails.score"></span> {{percent}}{{#percent}}<span class="unit" data-i18n="[html]units.percentage"></span>{{/percent}}</p>
                    <div class="table-responsive">
                        <table class="table table-striped table-hover table-condensed">
                            <!--
                                <thead class="hidden">
                                  <tr>
                                    <th>Country</th>
                                    <th class="text-right">Matching</th>
                                  </tr>
                                </thead>
                                -->
                            {{#matches}}
                            <tr>
                                <!--<td class="text-right hidden-xs"><span class="{{iconClass}}"></span></td>-->
                                <th><span data-i18n="priorities:{{id}}.title"></span></th>
                                <td class="col-sm-3 text-right {{scoreClass}}">
                                    {{percent}}{{#percent}}<span class="unit" data-i18n="[html]units.percentage"></span>{{/percent}}
                                    {{#warning}}<span class="glyphicon glyphicon-info-sign text-info" data-toggle="tooltip" data-i18n="[title]values:scoreLegends.warning"></span>{{/warning}}
                                    {{#error}}<span class="glyphicon glyphicon-warning-sign text-warning" data-toggle="tooltip" data-i18n="[title]values:scoreLegends.error"></span>{{/error}}
                                    <!--{{#icons}}<span class="{{iconClass}}" data-i18n="[title]values:scoreLegends.warning"></span>{{/icons}}-->
                                </td>
                            </tr>
                            {{/matches}}
                        </table>
                    </div>
                    <!--
                        <small>
                         <ul class="list-inline pull-right">
                           <li><span class="glyphicon glyphicon-info-sign text-info"></span> <span data-i18n="countryDetails.legend.warning"></span></li>
                           <li><span class="glyphicon glyphicon-warning-sign text-warning"></span> <span data-i18n="countryDetails.legend.error"></span></li>
                         </ul>
                        </small><span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>
                        -->
                    <!--
                        <ul class="list-unstyled list-condensed">
                          {{#matches}}
                          <li class="clearfix">
                            <span class="{{colorClass}}"><span class="{{iconClass}}"></span><small>{{percent}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span></small><span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span></span>&nbsp;
                            <span data-i18n="priorities.disabledPriorities.priorities.{{id}}.label"></span>
                          </li>
                          {{/matches}}
                        </ul>
                        -->
                </section>
                <section id="country_pane" class="tab-pane fade active in">
                    <!--<div class="clearfix">-->
                        <img src="{{flag.src}}" class="col-xs-4 pull-right img-rounded well well-sm" alt="{{Country}}" data-i18n="[alt]values:countries.{{ISO}}"/>
                        <!--<h4><span class="glyphicon glyphicon-globe"></span> <span data-i18n="sections:country.title"></span></h4>-->
                        <dl class="dl-horizontal col-md-8">
                            <dt data-i18n="fields:codes.label"></dt>
                            <dd>
                                <a href="http://en.wikipedia.org/wiki/ISO_3166-2:{{ISO}}" target="_blank" data-toggle="tooltip" title="ISO 3166-2">{{codes.ISO}}</a>
                                <a href="http://en.wikipedia.org/wiki/ISO_3166-1_alpha-3" target="_blank" data-toggle="tooltip" title="ISO 3166-1 alpha-3">{{codes.ISO3}}</a>
                                <a href="http://en.wikipedia.org/wiki/ISO_3166-1_numeric" target="_blank" data-toggle="tooltip" title="ISO 3166-1 numeric">{{codes.ISO-Numeric}}</a>
                                {{#codes.fips}}<a href="http://en.wikipedia.org/wiki/List_of_FIPS_country_codes" target="_blank" data-toggle="tooltip" title="FIPS 10-4">{{codes.fips}}</a>{{/codes.fips}}
                                {{#codes.EquivalentFipsCode}}<a href="http://en.wikipedia.org/wiki/List_of_FIPS_country_codes" target="_blank" data-toggle="tooltip" title="FIPS 10-4">{{codes.EquivalentFipsCode}}</a>{{/codes.EquivalentFipsCode}}
                            </dd>
                            {{#if continent.[Continent name]}}
                            <dt data-i18n="fields:continent.label"></dt>
                            <dd>{{continent.[Continent name]}}</dd>
                            {{/if}}
                            {{#if Capital}}
                            <dt data-i18n="fields:capital.label"></dt>
                            <dd><a href="https://www.google.fr/maps/place/{{Capital}}" target="_blank">{{Capital}}</a></dd>
                            {{/if}}
                            {{#if [Area(in sq km)]}}
                            <dt data-i18n="fields:area.label"></dt>
                            <dd>{{[Area(in sq km)]}}<span class="unit" data-i18n="[html]units.squareKilometers"></span></dd>
                            {{/if}}
                        <!--</dl>
                    </div>
                    <h4><span class="glyphicon glyphicon-usd"></span> <span data-i18n="sections:economy.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if currency.CurrencyName}}
                        <dt data-i18n="fields:officialCurrency.label"></dt>
                        <dd>{{currency.CurrencyName}} ({{currency.CurrencyCode}})</dd>
                        {{/if}}
                        {{#if userCurrencies}}
                        <dt data-i18n="fields:changeRate.label"></dt>
                        <dd>
                            <ul class="list-unstyled list-condensed">
                                {{#userCurrencies}}
                                {{#if localCurrencyValue}}
                                <li>
                                    1.00 <span class="unit">{{currencyName}}</span> = {{localCurrencyValue}} <span class="unit">{{localCurrencyName}}</span>
                                    <br/>1.00 <span class="unit">{{localCurrencyName}}</span> = {{currencyValue}} <span class="unit">{{currencyName}}</span>
                                    <a class="btn btn-default btn-xs pull-right" href="{{{changeRateURL}}}" target="_blank" data-i18n="buttons:check.label"></a>
                                </li>
                                {{/if}}
                                {{/userCurrencies}}
                            </ul>
                        </dd>
                        {{/if}}
                        {{#if gdp}}
                        <dt data-i18n="fields:grossDomesticProduct.label"></dt>
                        <dd>{{gdp}}<span class="unit" data-i18n="[html]units.usDollar"></span></dd>
                        {{/if}}
                    <!--</dl>
                    <h4><span class="glyphicon glyphicon-bishop"></span> <span data-i18n="sections:policy.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if democracy.Category}}
                        <dt data-i18n="fields:politicalRegimeType.label"></dt>
                        <dd data-i18n="values:politicalRegimeTypes.{{democracy.Category}}">{{democracy.Category}}</dd>
                        {{/if}}
                        {{#if democracy.Score}}
                        <dt data-i18n="fields:democracyScore.label"></dt>
                        <dd>{{democracy.Score}}<span class="unit">/10</span></dd>
                        {{/if}}
                    <!--</dl>-->
                    {{#if islands}}
                    <!--<h4><span class="glyphicon glyphicon-map-marker"></span> <span data-i18n="sections:territorialDependencies.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        <dt data-i18n="fields:islands.label"></dt>
                        <dd>
                            <ul class="list-inline">
                                {{#islands}}
                                <li><a href="http://en.wikipedia.org/?search={{name}}" target="_blank">{{name}}</a></li>
                                {{/islands}}
                            </ul>
                        </dd>
                    <!--</dl>-->
                    {{/if}}
                    </dl>
                </section>
                <section id="climate_pane" class="tab-pane fade">
                    <h4><span class="glyphicon glyphicon-cloud"></span> <span data-i18n="sections:climate.title"></span></h4>
                    <dl class="dl-horizontal">
                        <dt data-i18n="fields:weather.label"></dt>
                        <dd><a class="btn btn-default btn-xs" href="{{{climatURL}}}" target="_blank" data-i18n="buttons:check.label"></a></dd>
                    </dl>
                </section>
                <section id="population_pane" class="tab-pane fade">
                    <!--<h4><span class="glyphicon glyphicon-user"></span> <span data-i18n="sections:population.title"></span></h4>-->
                    <dl class="dl-horizontal">
                        {{#if population}}
                        <dt data-i18n="fields:population.label"></dt>
                        <dd>{{population}}</dd>
                        {{/if}}
                        {{#if literacyPercent}}
                        <dt data-i18n="fields:literacyPercent.label"></dt>
                        <dd>{{literacyPercent}}{{#literacyPercent}}<span class="unit" data-i18n="[html]units.percentage"></span>{{/literacyPercent}}</dd>
                        {{/if}}
                        {{#if incomeLevel.id}}
                        <dt data-i18n="fields:incomeLevel.label"></dt>
                        <dd data-i18n="values:incomeLevels.{{incomeLevel.id}}">{{incomeLevel.value}}</dd>
                        {{/if}}
                    <!--</dl>
                    <h4><span class="glyphicon glyphicon-comment"></span> <span data-i18n="sections:language.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        <dt data-i18n="fields:officialLanguages.label"></dt>
                        <dd>
                            <ul class="list-inline">
                                {{#languagePopulation}}
                                {{#if officialStatus}}
                                <li>
                                    {{[Language Name]}}
                                    <small>(<span data-i18n="fields:officialLanguages.{{officialStatus}}"></span>)</small>
                                </li>
                                {{/if}}
                                {{/languagePopulation}}
                            </ul>
                        </dd>
                        <dt data-i18n="fields:speakingLanguages.label"></dt>
                        <dd>
                            <ul class="list-inline">
                                {{#languagePopulation}}
                                {{#unless officialStatus}}
                                <li>
                                    {{[Language Name]}}{{#if populationPercent}} <small>({{populationPercent}}<span class="unit" data-i18n="[html]units.percentage"></span>)</small>{{/if}}
                                </li>
                                {{/unless}}
                                {{/languagePopulation}}
                            </ul>
                        </dd>
                    <!--</dl>
                    <h4><span class="glyphicon glyphicon-bookmark"></span> <span data-i18n="sections:culture.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if religions}}
                        <dt data-i18n="fields:religions.label"></dt>
                        <dd>
                            <ul class="list-inline">
                                {{#each religions}}
                                <li><span data-i18n="values:sides.{{name}}">{{name}}</span>{{#if percent}} <small>({{percent}}<span class="unit" data-i18n="[html]units.percentage"></span>)</small>{{/if}}</li>
                                {{/each}}
                            </ul>
                        </dd>
                        {{/if}}
                    </dl>
                </section>
                <section id="spending_pane" class="tab-pane fade">
                    <h4><span class="glyphicon glyphicon-shopping-cart"></span> <span data-i18n="sections:purchasingPower.title"></span></h4>
                    <dl class="dl-horizontal">
                        <dt data-i18n="fields:costOfLiving.label"></dt>
                        <dd><a class="btn btn-default btn-xs" href="{{{costOfLivingURL}}}" target="_blank" data-i18n="buttons:check.label"></a></dd>
                    </dl>
                </section>
                <section id="travel_pane" class="tab-pane fade">
                    <!--<h3 data-toggle="tooltip" data-i18n="[data-original-title]sections:tourism.info"><span class="glyphicon glyphicon-sunglasses"></span> <span data-i18n="sections:tourism.title"></span></h3>-->
                    {{#citizenships}} 
                    <!--<h4><span data-i18n="sections:citizenship.title"></span> <span data-i18n="values:countries.{{originCountryISO}}"></span></h4>-->
                    <dl class="dl-horizontal">
                        {{#if visaFreeLimit}}
                        <dt data-i18n="fields:visaFree.label"></dt>
                        <dd>
                            {{visaFreeLimit}} <span data-i18n="values:visaRequirements.visaFreeLimit"></span>
                            <a class="btn btn-default btn-xs pull-right" href="{{{url}}}" target="_blank" data-i18n="buttons:check.label"></a>
                        </dd>
                        {{/if}}
                        {{#if [voa-fee]}}
                        <dt data-i18n="fields:voaFee.label"></dt>
                        <dd>
                            {{[voa-fee]}}<span data-i18n="values:visaRequirements.voaFee"></span>
                        </dd>
                        {{/if}}
                        {{#if requirements}}
                        <dt data-i18n="fields:requiredOnArrival.label"></dt>
                        <dd>
                            <ul class="-list-inline">
                                {{#each requirements}}
                                <li>
                                    <span data-i18n="values:visaRequirements.{{this}}">{{this}}</span>
                                </li>
                                {{/each}}
                            </ul>
                        </dd>
                        {{/if}}
                    </dl>
                    {{/citizenships}} 
                </section>
                <section id="standards_pane" class="tab-pane fade">
                    <!--<h4><span class="glyphicon glyphicon-flash"></span> <span data-i18n="sections:acStandards.title"></span></h4>-->
                    <dl class="dl-horizontal">
                    {{#if plugs}}
                    <dt>
                        <span data-i18n="fields:residentialPlugs.label"></span>
                        <br />
                        <a class="btn btn-default btn-xs" href="{{{plugTypesURL}}}" target="_blank" data-i18n="buttons:check.label"></a>
                    </dt>
                    <dd>
                        <ul class="list-unstyled list-condensed clearfix">
                            {{#plugs}}
                            <li class="pull-left text-center">
                                <a href="images/plugs_sockets/{{plugType}}_3d_plug_L.png" target="_blank">
                                <img src="images/plugs_sockets/{{plugType}}_dia_sock_s.png" />
                                </a>
                                <a href="http://www.iec.ch/worldplugs/type{{plugType}}.htm" target="_blank">
                                <br />Type {{plugType}}</a>
                                <br />{{#each electricPotentials}}{{this}}<span class="unit" data-i18n="[html]units.volt"></span> {{/each}}
                                <br />{{#each frequencies}}{{this}}<span class="unit" data-i18n="[html]units.hertz"></span> {{/each}}
                            </li>
                            {{/plugs}}
                        </ul>
                    </dd>
                    {{/if}}
                    <!--<dl>
                    <h4><span class="glyphicon glyphicon-time"></span> <span data-i18n="sections:time.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if timeZones}}
                        <dt data-i18n="fields:timeZones.label"></dt>
                        <dd>
                            <ul class="list-unstyled">
                                {{#timeZones}}
                                <li>
                                    <span class="col-xs-7">{{timeZoneId}}</span>
                                    <span class="label label-primary">GMT {{#if-gt gmtOffset 0}}+{{/if-gt}}{{gmtOffset}}</span>
                                    <span class="label label-default">DST {{#if-gt dstOffset 0}}+{{/if-gt}}{{dstOffset}}</span>
                                    <span class="label label-default">RAW {{#if-gt rawOffset 0}}+{{/if-gt}}{{rawOffset}}</span>
                                </li>
                                {{/timeZones}}
                            </ul>
                        </dd>
                        {{/if}}
                    <!--</dl>
                    <h4><span class="glyphicon glyphicon-envelope"></span> <span data-i18n="sections:communication.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if Phone}}
                        <dt data-i18n="fields:phonePrefix.label"></dt>
                        <dd>{{Phone}}</dd>
                        {{/if}}
                        {{#if [Postal Code Format]}}
                        <dt data-i18n="fields:postalCodeFormat.label"></dt>
                        <dd>{{[Postal Code Format]}}</dd>
                        {{/if}}
                    <!--</dl>
                    <h4><span class="glyphicon glyphicon-road"></span> <span data-i18n="sections:driving.title"></span></h4>
                    <dl class="dl-horizontal">-->
                        {{#if drivingSide}}
                        <dt data-i18n="fields:drivingSide.label"></dt>
                        <dd data-i18n="values:sides.{{drivingSide}}">{{drivingSide}}</dd>
                        {{/if}}
                    </dl>
                </section>
                <section id="life_pane" class="tab-pane fade">
                    {{#bli}}
                    <form id="bli_form" class="sayt" data-persist="garlic" data-destroy="false" way-data="formData" way-persistent role="form">
                        <select id="bli_visible_column" name="bli_visible_column" class="pull-right">
                            <option value="2" data-i18n="fields:total.label" selected></option>
                            <option value="3" data-i18n="fields:men.label"></option>
                            <option value="4" data-i18n="fields:women.label"></option>
                            <option value="5" data-i18n="fields:highIncomes.label"></option>
                            <option value="6" data-i18n="fields:lowIncomes.label"></option>
                        </select>
                    </form>
                    {{/bli}}
                    {{#bli}}
                    <h4><span class="glyphicon glyphicon-home"></span> <span data-i18n="sections:housing.title"></span></h4>
                    <div class="table-responsive">
                        <table class="table table-hover table-condensed bli-table">
                            <colgroup>
                                <col class="col-md-5">
                                <!--<col span="1">-->
                                <col span="5" class="col-md-7">
                            </colgroup>
                            <thead class="hidden">
                                <th></th>
                                <th class="text-right" data-i18n="fields:total.label"></th>
                                <th class="text-right" data-i18n="fields:men.label"></th>
                                <th class="text-right" data-i18n="fields:women.label"></th>
                                <th class="text-right" data-i18n="fields:highIncomes.label"></th>
                                <th class="text-right" data-i18n="fields:lowIncomes.label"></th>
                            </thead>
                            <tbody>
                                {{#HO_BASE}}
                                <tr>
                                    <th>
                                        {{#TOT}}
                                        <span data-i18n="fields:{{INDICATOR}}.label"></span>
                                        {{#if [Reference Period]}} <small class="label label-default">{{[Reference Period]}}</small>{{/if}}
                                        {{#if [Flag Codes]}} <small class="label label-info" data-i18n="countryDetails.bli.flag.{{[Flag Codes]}}"></small>{{/if}}
                                        {{/TOT}}
                                    </th>
                                    <td class="text-right">{{#TOT}}<strong>{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span></strong>{{/TOT}}</td>
                                    <td class="text-right hidden-xs">{{#MN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/MN}}</td>
                                    <td class="text-right hidden-xs">{{#WMN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/WMN}}</td>
                                    <td class="text-right hidden-xs">{{#HGH}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/HGH}}</td>
                                    <td class="text-right hidden-xs">{{#LW}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/LW}}</td>
                                </tr>
                                {{/HO_BASE}}
                                {{#HO_HISH}}
                                <tr>
                                    <th>
                                        {{#TOT}}
                                        <span data-i18n="fields:{{INDICATOR}}.label"></span>
                                        {{#if [Reference Period]}} <small class="label label-default">{{[Reference Period]}}</small>{{/if}}
                                        {{#if [Flag Codes]}} <small class="label label-info" data-i18n="countryDetails.bli.flag.{{[Flag Codes]}}"></small>{{/if}}
                                        {{/TOT}}
                                    </th>
                                    <td class="text-right">{{#TOT}}<strong>{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span></strong>{{/TOT}}</td>
                                    <td class="text-right hidden-xs">{{#MN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/MN}}</td>
                                    <td class="text-right hidden-xs">{{#WMN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/WMN}}</td>
                                    <td class="text-right hidden-xs">{{#HGH}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/HGH}}</td>
                                    <td class="text-right hidden-xs">{{#LW}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/LW}}</td>
                                </tr>
                                {{/HO_HISH}}
                                {{#HO_NUMR}}
                                <tr>
                                    <th>
                                        {{#TOT}}
                                        <span data-i18n="fields:{{INDICATOR}}.label"></span>
                                        {{#if [Reference Period]}} <small class="label label-default">{{[Reference Period]}}</small>{{/if}}
                                        {{#if [Flag Codes]}} <small class="label label-info" data-i18n="values:bliFlags.{{[Flag Codes]}}"></small>{{/if}}
                                        {{/TOT}}
                                    </th>
                                    <td class="text-right">{{#TOT}}<strong>{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span></strong>{{/TOT}}</td>
                                    <td class="text-right hidden-xs">{{#MN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/MN}}</td>
                                    <td class="text-right hidden-xs">{{#WMN}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/WMN}}</td>
                                    <td class="text-right hidden-xs">{{#HGH}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/HGH}}</td>
                                    <td class="text-right hidden-xs">{{#LW}}{{Value}}<span class="unit" data-i18n="[html]units.{{[Unit Code]}}"></span>{{/LW}}</td>
                                </tr>
                                {{/HO_NUMR}}
                            </tbody>
                        </table>
                    </div>
                    {{/bli}}
                    {{#bli}}
                    <h4><span class="glyphicon glyphicon-education"></span> <span data-i18n="sections:education.title"></span></h4>
                    <div class="table-responsive">
                        <table class="table table-hover table-condensed bli-table">
                            <colgroup>
                                <col class="col-md-5">
                                <!--<col span="1">-->
                                <col span="5" class="col-md-7">
                            </colgroup>
                            <thead class="hidden">
                                <th></th>
                                <th class="text-right" data-i18n="fields:total.label"></th>
                                <th class="text-right" data-i18n="fields:men.label"></th>
                                <th class="text-right" data-i18n="fields:women.label"></th>
