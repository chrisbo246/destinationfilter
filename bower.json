  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.3",
    "bootstrap-chosen": "~1.0.1",
    "bootstrap-switch": "~3.3.2",
    "bootstrap-tour": "~0.10.2",
    "bootstrapvalidator": "~0.5.3",
    "bootswatch": "~3.3.5+2",
    "chosen": "~1.4.2",
    "fastclick": "~1.0.6",
    "i18next": "~1.10.2",
    "image-picker": "~0.2.4",
    "jquery": "~2.1.4",
    "jquery-array-utilities": "~1.0.0",
    "jquery-xml2json": "~0.0.8",
    "jquery.ui": "~1.11.4",
    "jqueryui-touch-punch": "git://github.com/furf/jquery-ui-touch-punch.git",
    "mixitup": "~2.1.9",
    "moment": "~2.10.6",
    "moment-timezone": "~0.4.0",
