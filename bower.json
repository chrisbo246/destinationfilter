{
  "name": "destinationfilter",
  "description": "Find the best destinations for your next trip.",
  "version": "0.7.3",
  "authors": [
    "christophe.boisier@live.fr"
  ],
  "private": true,
  "keywords": [
    "css",
    "js",
    "mobile-first",
    "responsive",
    "front-end",
    "web"
  ],
  "homepage": "http://christopheboisier.github.io/destinationfilter/",
  "main": [
    "dist/*"
